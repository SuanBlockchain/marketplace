{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@marketplaces/data-access": ["data-access/src/index.ts"],
      "@marketplaces/data-access-lib": ["data-access-lib/src/index.ts"],
      "@marketplaces/data-access-lib/server": ["data-access-lib/src/server.ts"],
      "@marketplaces/ui-lib": ["ui-lib/src/index.ts"],
      "@marketplaces/ui-lib/auth2": ["ui-lib/auth2/src/index.ts"],
      "@marketplaces/ui-lib/auth2/src/lib/*": ["ui-lib/auth2/src/lib/*"],
      "@marketplaces/ui-lib/src/lib/auth/*": ["ui-lib/src/lib/auth/*"],
      "@marketplaces/ui-lib/src/lib/buttons/*": ["ui-lib/src/lib/buttons/*"],
      "@marketplaces/ui-lib/src/lib/cardano-wallet/*": [
        "ui-lib/src/lib/cardano-wallet/*"
      ],
      "@marketplaces/ui-lib/src/lib/cards/*": ["ui-lib/src/lib/cards/*"],
      "@marketplaces/ui-lib/src/lib/common/*": ["ui-lib/src/lib/common/*"],
      "@marketplaces/ui-lib/src/lib/corewallet/*": [
        "ui-lib/src/lib/corewallet/*"
      ],
      "@marketplaces/ui-lib/src/lib/dashboard/*": [
        "ui-lib/src/lib/dashboard/*"
      ],
      "@marketplaces/ui-lib/src/lib/epayco/*": ["ui-lib/src/lib/epayco/*"],
      "@marketplaces/ui-lib/src/lib/graphs/*": ["ui-lib/src/lib/graphs/*"],
      "@marketplaces/ui-lib/src/lib/icons/*": ["ui-lib/src/lib/icons/*"],
      "@marketplaces/ui-lib/src/lib/landing/*": ["ui-lib/src/lib/landing/*"],
      "@marketplaces/ui-lib/src/lib/layout/*": ["ui-lib/src/lib/layout/*"],
      "@marketplaces/ui-lib/src/lib/modals/*": ["ui-lib/src/lib/modals/*"],
      "@marketplaces/ui-lib/src/lib/store/*": ["ui-lib/src/lib/store/*"],
      "@marketplaces/ui-lib/src/lib/trade/*": ["ui-lib/src/lib/trade/*"],
      "@marketplaces/ui-lib/src/lib/wallet/*": ["ui-lib/src/lib/wallet/*"],
      "@marketplaces/utils-2": ["utils-2/src/index.ts"],
      "@marketplaces/utils-2/src/lib/context/*": ["utils-2/src/lib/context/*"],
      "@marketplaces/utils-2/src/lib/dicc": ["utils-2/src/lib/dicc.tsx"],
      "@marketplaces/utils-2/src/lib/generic/*": ["utils-2/src/lib/generic/*"],
      "@marketplaces/utils-2/src/lib/mappers/*": ["utils-2/src/lib/mappers/*"],
      "@suan/*": ["apps/suan-marketplace/*"],
      "@terrasacha/*": ["apps/terrasacha-marketplace/*"],
      "@cauca/*": ["apps/suan-cauca-marketplace/*"],
      "@marketplaces/*": ["./*"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
